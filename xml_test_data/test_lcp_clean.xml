      <item>2.5m/min</item>         <!-- item_9: seq=4 速度 -->
      <item>15.5</item>             <!-- item_10: seq=5 LCP量 -->
    </row>

    <!-- ====================== -->
    <!-- 第3行：加工条件种cd=003 -->
    <!-- 用途：工艺说明 -->
