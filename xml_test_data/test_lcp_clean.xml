      <item>V1.0</item>             <!-- item_10: seq=2, multiKey=4 版本 -->
      <item>25.5</item>             <!-- item_11: seq=2, multiKey=5 温度 -->
      <item>60</item>               <!-- item_12: seq=2, multiKey=6 速度 -->
