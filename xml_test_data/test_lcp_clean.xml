      <item>65</item>
    </row>

    <!-- 工程系列B的数据 -->
    <row>
      <item>02</item>
      <item>LCP-2024-A</item>
      <item>工程系列X</item>
      <item>001</item>
      <item>N01</item>
      <item>001</item>
      <item>LCP-2024-A</item>
