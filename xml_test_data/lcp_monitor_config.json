            },
            {
              "subName": "W",
              "unit": "um",
              "dataSource": "database",
              "displayType": "direct",
              "valueKey": "precisionW",
              "expression": "SELECT precision_w FROM lcp_dimension WHERE lcp_name = :lcpName AND version = :version"
            },
            {
              "subName": "H",
              "unit": "um",
              "dataSource": "database",
              "displayType": "direct",
              "valueKey": "precisionH",
              "expression": "SELECT precision_h FROM lcp_dimension WHERE lcp_name = :lcpName AND version = :version"
            }
          ]
        }
      ]
    }
