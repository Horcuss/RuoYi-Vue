      "dataSource": "database",
      "displayType": "direct",
      "valueKey": "qualityReq",
      "expression": "SELECT quality_requirement FROM lcp_standards WHERE lcp_name = :lcpName"
    }
  ],
  "tableConfigs": [
    {
      "rowHeader": "印刷检查",
      "rows": [
        {
          "rowType": "simple",
          "projectName": "LCP处置项次",
          "unit": "",
          "dataSource": "xml",
          "displayType": "direct",
          "valueKey": "disposalTimes",
          "expression": "002;1;1"
        },
        {
          "rowType": "simple",
          "projectName": "印刷方式",
          "unit": "",
          "dataSource": "xml",
          "displayType": "direct",
          "valueKey": "printMethod",
          "expression": "002;1;2"
